{
  "expo": {
    "name": "Task Master",
    "slug": "Task-Master",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/iOSIcon.PNG",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tweedtechservices.tmapp",
      "buildNumber": "1.0.3",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app will optionally use Face ID or Touch ID to save login"
      }
    },
    "android": {
      "package": "com.tweedtechservices.tmapp",
      "versionCode": 1,
      "permissions" : []
    },
    "description": "Organization on the go.  Manage your task list from your phone so you are always on top of things.",
    "githubUrl": "https://github.com/ptweed/TMApp",
    "primaryColor": "#ffffff"
  }
}
